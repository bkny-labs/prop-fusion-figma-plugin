{
  "name": "design-system-sync-figma-plugin",
  "version": "1.0.0",
  "description": "PropFusion Figma plugin is used to sync your team's Figma design system from Figma your component library codebase.",
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=development --watch",
    "prettier:format": "prettier --write '**/*.{js,jsx,ts,tsx,css,json}' "
  },
  "dependencies": {
    "monaco-editor": "^0.44.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.1.0",
    "react-monaco-editor": "^0.55.0",
    "react-syntax-highlighter": "^15.5.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.50.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.8",
    "react-dev-utils": "^12.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
